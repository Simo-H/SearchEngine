<Window x:Class="SearchEngine.ResultsAllQueries"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchEngine"
        mc:Ignorable="d"
        Title="Queries Results" Height="500" Width="500" ResizeMode="NoResize">
    <Grid Margin="0,0,0,3">
        <ListView x:Name ="listview" Margin="0,5,0,45" ItemsSource="{Binding QueriesResults}" />
        <Button x:Name="button" Content="ok" HorizontalAlignment="Left" Height="25" Margin="213,200,0,15" VerticalAlignment="Bottom" Width="71" Click="button_Click"/>

    </Grid>
</Window>
