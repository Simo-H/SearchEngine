<Window x:Class="SearchEngine.ShowRankResults"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchEngine"
        mc:Ignorable="d"
        Title="ShowRankResults" Height="250" Width="150">
    
    <Grid Margin="0,0,0,-21">
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="250" VerticalAlignment="Top" Width="145">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Doc number" Width="150" />
                </GridView>
            </ListView.View>
        </ListView>
        <Button HorizontalAlignment="Left" Name="Ok"
                    
                    
                    Content="OK"
                    Height="25"
                    Width="90"
                    Margin ="97,245,0,30" Click="ok"></Button>
    </Grid>
</Window>
