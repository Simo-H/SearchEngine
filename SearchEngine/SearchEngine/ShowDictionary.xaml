<Window x:Class="SearchEngine.ShowDictionary"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchEngine"
        mc:Ignorable="d"
        Title="ShowDictionary" Height="310" Width="255">
    <Grid Margin="0,0,0,-21">
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="240" VerticalAlignment="Top" Width="235">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Term" Width="135"  DisplayMemberBinding="{Binding Term}"/>
                    <GridViewColumn Header="CF" Width="70" DisplayMemberBinding="{Binding CF}"/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button HorizontalAlignment="Left" Name="Ok"
                    
                    
                    Content="OK"
                    Height="25"
                    Width="90"
                    Margin ="74,245,0,30" Click="ok"></Button>
    </Grid>
    
</Window>
